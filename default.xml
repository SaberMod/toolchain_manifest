<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="aosp"
           fetch="https://android.googlesource.com/toolchain/"
           review="https://android-review.googlesource.com/" />
  <remote  name="sm"
           fetch="http://github.com/SaberMod/" />
  <remote  name="gnu-bin-gdb"
           fetch="git://sourceware.org/git/" />
  <default revision="master"
           remote="aosp"
           sync-j="4"
           sync-c="true" />

  <project path="manifest" name="toolchain_manifest" remote="sm" revision="master" />
  <project path="avr-libc" name="avr-libc" />
  <project path="benchmark" name="benchmark" />
  <project path="binutils/binutils-gnu" name="binutils-gdb" remote="gnu-bin-gdb" revision="binutils-2_24-branch" />
  <project path="build" name="toolchain_build" remote="sm" revision="master" />
  <project path="ccache" name="ccache" />
  <project path="clang" name="clang" />
  <project path="cloog" name="cloog" />
  <project path="expat" name="expat" />
  <project path="gcc/gcc-SaberMod" name="GCC_SaberMod" remote="sm" revision="4.9.0" />
  <project path="gdb/gdb-gnu" name="binutils-gdb" remote="gnu-bin-gdb" revision="gdb-7.7-branch" />
  <project path="gmp/gmp-gnu" name="gnu-gmp" remote="sm" revision="5.1.3" />
  <project path="gold" name="gold" />
  <project path="llvm" name="llvm" />
  <project path="mclinker" name="mclinker" />
  <project path="mpc/mpc-gnu" name="gnu-mpc" remote="sm" revision="1.0.2" />
  <project path="mpfr/mpc-gnu" name="gnu-mpfr" remote="sm" revision="3.1.2" />
  <project path="perl" name="perl" />
  <project path="polly" name="polly" />
  <project path="ppl" name="ppl" />
  <project path="python" name="python" />

</manifest>
